{
  "name": "@sixui/ui",
  "description": "UI components",
  "version": "0.0.74",
  "main": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/styles/main.css",
    "./*.stylex": "./dist/*.stylex.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "git+https://github.com/sixui/ui.git"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18",
    "yarn": "4.1.0"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "dev": "BROWSER=none storybook dev -p 6006 --no-open --disable-telemetry",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-storybook": "yarn build:sb",
    "build:sb": "storybook build",
    "build:ts": "babel src --out-dir dist --config-file ./.babelrc --extensions '.ts,.tsx' --delete-dir-on-start --copy-files",
    "build:dts": "tsc --emitDeclarationOnly && tsc-alias",
    "build": "run-s build:ts build:dts",
    "release": "auto shipit",
    "prettify:fix:file": "prettier --write",
    "lint:fix:file": "eslint --fix",
    "lint:check": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "ts:check": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "run-s lint:check ts:check"
  },
  "lint-staged": {
    "src/**/*.(ts|js)?(x)": [
      "yarn prettify:fix:file",
      "yarn lint:fix:file"
    ]
  },
  "devDependencies": {
    "@auto-it/npm": "^11.1.1",
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/builder-vite": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@stylexjs/eslint-plugin": "^0.5.1",
    "@stylexjs/stylex": "^0.5.1",
    "@types/eslint": "^8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "modern-normalize": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.17",
    "storybook-addon-pseudo-states": "^2.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-stylex": "^0.5.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "packageManager": "yarn@4.1.0",
  "readme": "README.md",
  "dependencies": {
    "@olivierpascal/helpers": "^0.0.67"
  },
  "peerDependencies": {
    "@stylexjs/stylex": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
