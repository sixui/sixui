{
  "name": "@sixui/ui",
  "description": "Ready-to-use foundational React components with near zero-runtime style injection.",
  "version": "0.0.404",
  "author": "Olivier Pascal <olivier@pascal.re>",
  "main": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/styles/main.css",
    "./*.stylex": "./dist/*.stylex.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "git+https://github.com/sixui/sixui.git"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "clean": "rimraf ./dist",
    "preinstall": "npx only-allow pnpm",
    "generate-icons:convert": "svg-to-ts-files",
    "generate-icons:format": "eslint src/assets/icons --ext .js,.ts,.jsx,.tsx --fix",
    "generate-icons": "run-s generate-icons:convert generate-icons:format",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "dev": "BROWSER=none storybook dev -p 6006 --no-open --disable-telemetry",
    "test": "run-s test:cy",
    "test:cy": "cypress run --component",
    "build-storybook": "pnpm build:sb",
    "build:sb": "storybook build",
    "build:ts": "tsc --project tsconfig.app.build.json && tsc-alias --project tsconfig.app.build.json",
    "build:copy-files": "cp -R src/styles dist/",
    "build": "run-s build:ts build:copy-files",
    "release": "auto shipit",
    "prettify:fix:file": "prettier --write",
    "lint:fix:file": "eslint --fix",
    "lint:check": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "ts:check": "tsc --project tsconfig.app.json --pretty --noEmit",
    "check:all": "run-s lint:check ts:check test",
    "cy": "cypress open"
  },
  "lint-staged": {
    "src/**/*.(ts|js)?(x)": [
      "pnpm prettify:fix:file",
      "pnpm lint:fix:file"
    ]
  },
  "devDependencies": {
    "@auto-it/npm": "^11.2.0",
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@storybook/addon-actions": "^8.2.7",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/builder-vite": "^8.2.7",
    "@storybook/react": "^8.2.7",
    "@storybook/react-vite": "^8.2.7",
    "@storybook/testing-library": "^0.2.2",
    "@stylexjs/eslint-plugin": "^0.7.5",
    "@stylexjs/stylex": "^0.7.5",
    "@types/eslint": "^8",
    "@types/node": "^22.1.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-transition-group": "^4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "auto": "^11.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^13.13.2",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.7",
    "svg-to-ts": "^12.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4",
    "validate-branch-name": "^1.3.1",
    "vite": "^5",
    "vite-plugin-stylex": "^0.10.1"
  },
  "packageManager": "pnpm@9.6.0",
  "readme": "README.md",
  "dependencies": {
    "@floating-ui/react": "^0.26.20",
    "@material/material-color-utilities": "^0.3.0",
    "@olivierpascal/helpers": "^0.0.71",
    "highlight-words": "^1.2.2",
    "modern-normalize": "^2.0.0",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "@stylexjs/stylex": "^0.7.5",
    "react": "^18"
  },
  "svg-to-ts": {
    "conversionType": "files",
    "generateType": true,
    "typeName": "IIcons",
    "srcFiles": [
      "./src/assets/*.svg"
    ],
    "iconsFolderName": "lib",
    "outputDirectory": "./src/assets/icons",
    "interfaceName": "IIcon",
    "prefix": "icon",
    "svgoConfig": {
      "plugins": [
        "cleanupAttrs"
      ]
    },
    "fileName": "icons.model"
  }
}
