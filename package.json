{
  "name": "@sixui/ui",
  "version": "0.0.465",
  "description": "Ready-to-use foundational React components with near zero-runtime style injection.",
  "repository": {
    "url": "git+https://github.com/sixui/sixui.git"
  },
  "license": "MIT",
  "author": "Olivier Pascal <olivier@pascal.re>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "clean": "rimraf ./dist",
    "preinstall": "npx only-allow pnpm",
    "generate-icons:convert": "svg-to-ts-files",
    "generate-icons:format": "eslint src/assets/icons --ext .js,.ts,.jsx,.tsx --fix",
    "generate-icons": "run-s generate-icons:convert generate-icons:format",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "dev": "BROWSER=none storybook dev -p 6006 --no-open --disable-telemetry",
    "test": "run-s test:cy",
    "test:cy": "cypress run --component",
    "build-storybook": "pnpm build:sb",
    "build:sb": "storybook build",
    "build:ts": "tsc --project tsconfig.app.build.json && tsc-alias --project tsconfig.app.build.json",
    "build:copy-files": "cp -R src/styles dist/",
    "build": "run-s build:ts build:copy-files",
    "release": "auto shipit",
    "prettify:fix:file": "prettier --write",
    "lint:fix:file": "eslint --fix",
    "lint:check": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "ts:check": "tsc --project tsconfig.app.json --pretty --noEmit",
    "check:all": "run-s lint:check ts:check test",
    "cy": "cypress open"
  },
  "lint-staged": {
    "**/*.(ts|js)?(x)": [
      "pnpm prettify:fix:file",
      "pnpm lint:fix:file"
    ]
  },
  "dependencies": {
    "@floating-ui/core": "^1.6.8",
    "@floating-ui/react": "^0.27.2",
    "@material/material-color-utilities": "^0.3.0",
    "@olivierpascal/helpers": "^0.0.71",
    "@vanilla-extract/css": "^1.17.0",
    "@vanilla-extract/css-utils": "^0.1.4",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "clsx": "^2.1.1",
    "highlight-words": "^2.0.0",
    "modern-normalize": "^3.0.1",
    "rainbow-sprinkles": "^1.0.0",
    "react-aria": "^3.36.0",
    "react-aria-components": "^1.5.0",
    "react-remove-scroll": "^2.6.2",
    "react-transition-group": "^4.4.5",
    "type-fest": "^4.31.0"
  },
  "devDependencies": {
    "@auto-it/npm": "^11.3.0",
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "^3",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@react-types/shared": "^3.26.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@stylexjs/eslint-plugin": "^0.9.3",
    "@stylexjs/stylex": "^0.9.3",
    "@types/eslint": "^9",
    "@types/node": "^22.10.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "^4",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vanilla-extract/vite-plugin": "^4.0.19",
    "@vitejs/plugin-react": "^4.3.4",
    "auto": "^11.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^13.17.0",
    "cypress-real-events": "^1.13.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.7",
    "svg-to-ts": "^12.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "validate-branch-name": "^1.3.1",
    "vite": "^6",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-stylex": "^0.13.0"
  },
  "peerDependencies": {
    "@stylexjs/stylex": "^0.7.5",
    "react": "^18"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "readme": "README.md"
}