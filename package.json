{
  "name": "@sixui/ui",
  "description": "UI components",
  "version": "0.0.264",
  "author": "Olivier Pascal <olivier@pascal.re>",
  "main": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/styles/main.css",
    "./*.stylex": "./dist/*.stylex.js"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "git+https://github.com/sixui/sixui.git"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18",
    "yarn": "4.1.0"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "dev": "BROWSER=none storybook dev -p 6006 --no-open --disable-telemetry",
    "test": "run-s test:cy",
    "test:cy": "cypress run --component",
    "build-storybook": "yarn build:sb",
    "build:sb": "storybook build",
    "build:ts": "babel src --out-dir dist --config-file ./.babelrc.build.json --extensions '.ts,.tsx' --delete-dir-on-start --copy-files --no-copy-ignored",
    "build:dts": "tsc --project tsconfig.build.json && tsc-alias",
    "build": "run-s build:ts build:dts",
    "release": "auto shipit",
    "prettify:fix:file": "prettier --write",
    "lint:fix:file": "eslint --fix",
    "lint:check": "eslint src --ext .js,.ts,.jsx,.tsx --fix",
    "ts:check": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "run-s lint:check ts:check test",
    "cy": "cypress open"
  },
  "lint-staged": {
    "src/**/*.(ts|js)?(x)": [
      "yarn prettify:fix:file",
      "yarn lint:fix:file"
    ]
  },
  "devDependencies": {
    "@auto-it/npm": "^11.1.6",
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@storybook/addon-actions": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/builder-vite": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/testing-library": "^0.2.2",
    "@stylexjs/eslint-plugin": "^0.7.5",
    "@stylexjs/stylex": "^0.7.5",
    "@types/eslint": "^8",
    "@types/lodash": "^4",
    "@types/node": "^20.14.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-transition-group": "^4",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "auto": "^11.1.6",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "cypress": "^13.13.0",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.11",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.3",
    "validate-branch-name": "^1.3.0",
    "vite": "^5",
    "vite-plugin-stylex": "^0.10.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "packageManager": "yarn@4.1.0",
  "readme": "README.md",
  "dependencies": {
    "@floating-ui/react": "^0.26.19",
    "@olivierpascal/helpers": "^0.0.70",
    "highlight-words": "^1.2.2",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "react-transition-group": "^4.4.5",
    "usehooks-ts": "^3.1.0"
  },
  "peerDependencies": {
    "@stylexjs/stylex": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
