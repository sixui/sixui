{
  "name": "@sixui/prettier-config",
  "description": "Prettier config for Sixui",
  "version": "2.16.0",
  "homepage": "https://sixui.com",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/sixui/sixui.git",
    "type": "git",
    "directory": "packages/eslint-config"
  },
  "author": "Olivier Pascal <olivier@pascal.re>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/defaultConfig.d.ts",
        "default": "./dist/defaultConfig.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "scripts": {
    "lint:check": "eslint . --fix",
    "ts:check": "tsc --pretty --noEmit",
    "check:all": "run-s lint:check ts:check",
    "clean": "rm -rf dist",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json"
  },
  "dependencies": {
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1"
  },
  "devDependencies": {
    "@sixui/eslint-config": "workspace:*",
    "@sixui/typescript-config": "workspace:*",
    "@types/node": "^22.13.16",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "tsc-alias": "^1.8.13"
  }
}
